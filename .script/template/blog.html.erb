    <link rel=stylesheet property=stylesheet type=text/css href=<%= version("https://www.jxck.io/assets/css/info.css") %>>
    <dl class=info>
      <div><dt>created_at</dt><dd><time class=created_at datetime=<%= entry.created_at %>><%= entry.created_at %></time></dd></div>
      <div><dt>updated_at</dt><dd><time class=updated_at datetime=<%= entry.updated_at %>><%= entry.updated_at %></time></dd></div>
      <div>
        <dt>tags</dt>
        <dd>
          <nav class=tags>
            <ul>
              <%- entry.tags.each{|tag| -%>
              <li><a href="/tags/<%=tag.gsub(' ', '%20')%>.html"><%=tag%></a>
              <%- } -%>
            </ul>
          </nav>
        </dd>
      </div>
      <div>
        <dt>toc</dt>
        <dd>
          <details class=info>
            <summary>headdings</summary>
            <nav>
              <ul>
                <%- entry.toc.each{|href, value| -%>
                <li><a href=<%= href %>><%= value %></a>
                <%- } -%>
              </ul>
            </nav>
          </details>
        </dd>
      </div>
    </dl>


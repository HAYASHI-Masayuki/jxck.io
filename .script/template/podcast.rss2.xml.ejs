<% episodes.forEach((episode) => { %>
    <item>
      <category>mozaicfm</category>
      <enclosure url="<%= episode.audio.replace('https://', 'http://') %>" length="<%= episode.audio_size %>" type="audio/mpeg" />
      <guid isPermaLink="false"><%= episode.canonical %></guid>
      <link><%= episode.canonical %></link>
      <pubDate><%= new Date(episode.published_at).toUTCString().replace("GMT", "+0900") %></pubDate>
      <title><%- episode.title %> | mozaic.fm</title>
      <itunes:author>Jxck</itunes:author>
      <itunes:duration><%= episode.duration %></itunes:duration>
      <itunes:explicit>no</itunes:explicit>
      <itunes:keywords>web,tech,it</itunes:keywords>
      <itunes:order><%= episode.order %></itunes:order>
      <itunes:subtitle><%- episode.description.split('\n')[0] %></itunes:subtitle>
      <media:content url="<%= episode.audio %>" fileSize="<%= episode.audio_size %>" type="audio/mpeg" />
      <description>
<%- episode.description %>

Show Note はこちら: <%= episode.canonical %>
      </description>
    </item>
<% }) %>
